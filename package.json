{
  "name": "react-with-timer",
  "version": "0.1.0",
  "description": "React HOC that simplifies the use of setTimeout-based timers.",
  "main": "./dist/lib.js",
  "files": [
    "config",
    "dist",
    "src",
    ".babelrc",
    "README.md",
    "webpack.config.js"
  ],
  "directories": {
    "config": "./config",
    "dist": "./dist",
    "src": "./src",
    "lib": "./src/lib"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=DEV webpack --progress --colors",
    "build:prod": "cross-env NODE_ENV=PROD webpack --progress --colors",
    "build:watch": "cross-env NODE_ENV=DEV webpack --progress --colors --watch",
    "clean": "rimraf dist/*",
    "modules": "rimraf node_modules/* && npm i",
    "test": "cross-env NODE_ENV=TEST mocha-webpack",
    "test:watch": "cross-env NODE_ENV=TEST mocha-webpack --watch",
    "prepublish": "npm run build:prod",
    "lint": "standard",
    "lint:fix": "standard --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jcheroske/react-with-timer.git"
  },
  "keywords": [
    "babel",
    "es2015",
    "es2015+",
    "es6",
    "webpack",
    "webpack 2",
    "npm",
    "yarn",
    "build",
    "publishing",
    "boilerplate"
  ],
  "author": "Jay Cheroske <jay@portal.org>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/resisttheurge/react-with-timer/issues"
  },
  "homepage": "https://github.com/jcheroske/react-with-timere#readme",
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-stage-0": "^6.22.0",
    "bind-infix-proxy": "^1.0.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.1",
    "cross-env": "^3.1.4",
    "enzyme": "^2.7.1",
    "jsverify": "^0.7.4",
    "lodash-webpack-plugin": "^0.11.2",
    "mocha": "^3.2.0",
    "mocha-webpack": "^0.7.0",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "rimraf": "^2.6.1",
    "sinon": "^1.17.7",
    "source-map-support": "^0.4.11",
    "standard": "^9.0.1",
    "webpack": "^2.2.1"
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "DEV",
      "PROD",
      "TEST"
    ]
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "react": "^15.4.2"
  }
}
